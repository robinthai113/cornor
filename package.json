{
  "name": "cucumber-playwright",
  "version": "1.0.0",
  "description": "BDD with Playwright",
  "main": "index.js",
  "scripts": {
    "all": "cucumber-js features/**/*.feature",
    "test": "cucumber-js",
    "test:parallel": "npm run clean-report && cucumber-js --parallel=1",
    "allure": "allure serve reports/allure-results",
    "test:tag": "npm run clean-report && cucumber-js --tags",
    "runalltest": "node run-test.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier-all": "npx prettier --write \"./src/**/*.ts{,x}\"",
    "clean-report": "node clean-report.js"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@cucumber/cucumber": "9.1.2",
    "@cucumber/html-formatter": "20.3.1",
    "@cucumber/messages": "^22.0.0",
    "@cucumber/pretty-formatter": "1.0.0",
    "@types/fs-extra": "11.0.1",
    "allure-commandline": "^2.22.4",
    "artillery": "^1.7.9",
    "async": "^3.2.5",
    "copy-paste": "^1.5.3",
    "cucumber-console-formatter": "1.0.0",
    "cucumber-html-reporter": "7.1.0",
    "date-fns": "2.30.0",
    "dotenv": "^16.3.1",
    "mssql": "^10.0.2",
    "node-fetch": "^3.3.2",
    "pixelmatch": "5.3.0",
    "pngjs": "7.0.0",
    "sharp": "^0.33.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.35.1",
    "@types/copy-paste": "^1.1.33",
    "@types/expect": "24.3.0",
    "@types/fs-extra": "11.0.1",
    "@types/lodash": "4.14.195",
    "@types/mssql": "^9.1.5",
    "@types/node": "18.16.18",
    "@types/pixelmatch": "5.2.4",
    "@types/pngjs": "6.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "allure-cucumberjs": "^2.4.0",
    "allure-js-commons": "^2.4.0",
    "allure-playwright": "^2.4.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "fs-extra": "11.1.1",
    "multiple-cucumber-html-reporter": "^3.5.0",
    "open": "9.1.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "standard-version": "9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  }
}
